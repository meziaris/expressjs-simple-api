{
  "name": "expressjs-simple-api",
  "version": "1.0.0",
  "description": "Simple API using ExpressJS",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit -p tsconfig.json"
