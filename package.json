    "simple",
    "typescript"
  ],
  "author": "apronny",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
