{
  "name": "expressjs-simple-api",
  "version": "1.0.0",
  "description": "Simple API using ExpressJS",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "tsx watch src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "expressjs",
    "api",
    "simple",
    "typescript"
  ],
  "author": "apronny",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@prisma/client": "6.14.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prisma": "^6.14.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
